{
  "baseURL": "https://www.tejalshinde.com/",
  "build": {
    "noJSConfigInAssets": true
  },
  "copyright": "Â© 2024 Tejal Shinde",
  "disableHugoGeneratorInject": true,
  "disableKinds": [
    "rss",
    "taxonomy",
    "term"
  ],
  "enableRobotsTxt": true,
  "imaging": {
    "exif": {
      "disableDate": true,
      "disableLatLong": true
    },
    "quality": 100
  },
  "minify": {
    "minifyOutput": true,
    "tdewolff": {
      "html": {
        "keepConditionalComments": false,
        "keepDefaultAttrVals": false,
        "keepEndTags": false,
        "keepWhitespace": false
      }
    }
  },
  "noBuildLock": true,
  "printPathWarnings": true,
  "printUnusedTemplates": true,
  "server": {
    "headers": [{
      "for": "/*",
      "values": {
        "content-security-policy": "frame-src www.google.com; media-src 'self' blob:; style-src 'self' 'unsafe-inline';"
      }
    }]
  },
  "templateMetrics": true,
  "templateMetricsHints": true,
  "timeout": 120000
}